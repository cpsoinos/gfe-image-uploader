{
  "name": "gfe-image-uploader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "build:next-on-pages": "npx @cloudflare/next-on-pages",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply gfe-image-uploader --local",
    "db:migrate:prod": "op run --env-file=\"./.env.production.local\" -- drizzle-kit migrate",
    "db:studio:local": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:prod": "op run --env-file=\"./.env.production.local\" -- drizzle-kit studio",
    "dev": "op run --env-file=\".env.local\" -- next dev",
    "format": "prettier --write .",
    "lint": "oxlint && next lint",
    "lint:fix": "oxlint --fix . && next lint --fix",
    "prepare": "husky",
    "preview": "pnpm wrangler pages dev .vercel/output/static --compatibility-flag nodejs_compat",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "wrangler": "op plugin run -- wrangler"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.624.0",
    "@aws-sdk/s3-request-presigner": "^3.624.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.32.2",
    "merge-refs": "^1.3.0",
    "next": "14.2.5",
    "posthog-js": "^1.154.5",
    "react": "^18",
    "react-dom": "^18",
    "react-image-crop": "^11.0.6",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@cloudflare/next-on-pages": "^1.13.1",
    "@cloudflare/workers-types": "^4.20240725.0",
    "@cpsoinos/prettier-config": "^1.0.0",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-viewport": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.14.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "better-sqlite3": "^11.1.2",
    "drizzle-kit": "^0.23.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-next-on-pages": "^1.13.1",
    "eslint-plugin-oxlint": "^0.5.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.0.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "msw": "^2.3.5",
    "msw-storybook-addon": "^2.0.3",
    "oxlint": "^0.6.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sort-package-json": "^2.10.0",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "wrangler": "^3.69.1"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
